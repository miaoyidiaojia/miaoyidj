<template name="5b1c92fc">
  <view class="_div data-v-e8c2c6ac">
    <van-cell-group class="_van-cell-group data-v-e8c2c6ac">
      <van-field value="{{userName}}" label="联系人" placeholder="姓名" bindchange="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_van-field data-v-e8c2c6ac"></van-field>
      <van-field value="{{userPhone}}" type="number" label="手机号" placeholder="将通过此手机联系你" bindchange="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_van-field data-v-e8c2c6ac"></van-field>
      <van-cell title="地址" is-link bindtap="handleProxy" value="{{addressVal}}" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_van-cell data-v-e8c2c6ac"></van-cell>
      <van-popup show="{{showBottom}}" position="bottom" custom-class="bottom" bindclose="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_van-popup data-v-e8c2c6ac">
        <van-picker show-toolbar title="从成都市区选择" columns="{{column1}}" bindchange="handleProxy" bindconfirm="handleProxy" bindcancel="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_van-picker data-v-e8c2c6ac"></van-picker>
      </van-popup>
      <van-field value="{{addressDtl}}" placeholder="详细地址" border="{{borderVal}}" bindchange="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}" class="_van-field data-v-e8c2c6ac"></van-field>
    </van-cell-group>
    <view style="height: 50rpx" class="_div data-v-e8c2c6ac"></view>
    <view wx:if="{{isAdd}}" class="_div data-v-e8c2c6ac">
      <van-button bindtap="handleProxy" size="large" type="danger" data-eventid="{{'6'}}" data-comkey="{{$k}}" class="_van-button data-v-e8c2c6ac">保存</van-button>
    </view>
    <view class="_div data-v-e8c2c6ac bu" wx:else>
      <van-button bindtap="handleProxy" type="default" data-eventid="{{'7'}}" data-comkey="{{$k}}" class="_van-button data-v-e8c2c6ac">删除</van-button>
      <van-button bindtap="handleProxy" type="danger" data-eventid="{{'8'}}" data-comkey="{{$k}}" class="_van-button data-v-e8c2c6ac">保存</van-button>
    </view>
    <van-toast id="van-toast" class="_van-toast data-v-e8c2c6ac"></van-toast>
  </view>
