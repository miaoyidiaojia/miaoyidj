<template name="cc91e49e">
  <view class="_div data-v-3f5c3f25 review-info">
    <view class="_div data-v-3f5c3f25 review-info-name">
      <view class="_div data-v-3f5c3f25 review-info-avatar">
        <i-avatar src="{{review.user.uavatar}}" size="large" shape="circle" class="_i-avatar data-v-3f5c3f25"></i-avatar>
      </view>
      <view class="_div data-v-3f5c3f25 review-info-rate">
        <view class="_div data-v-3f5c3f25 rate-name">{{review.user.uname}}</view>
        <i-rate disabled="{{dis}}" value="{{review.rrate}}" class="_i-rate data-v-3f5c3f25"></i-rate>
      </view>
      <view class="_div data-v-3f5c3f25 review-info-time">{{review.rreviewTime}}</view>
    </view>
    <view class="_div data-v-3f5c3f25 review-content">{{review.rcontent}}</view>
  </view>
</template>