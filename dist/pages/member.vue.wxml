<template name="65df532f">
  <view class="_div data-v-350cb141">
    <view class="_div data-v-350cb141 info-top">
      <view class="_div data-v-350cb141 info-top-content">
        <view class="_div data-v-350cb141 info-top-content-tx">
          <view class="_div data-v-350cb141 touxiang">
            <view class="_div data-v-350cb141 tupian">
              <image style="width: 90%;height: 90%;border-radius: 50px" src="{{miaoyiUser.uavatar}}" class="_img data-v-350cb141"></image>
            </view>
            <view class="_div data-v-350cb141 na">
              <view style="width: 90%;height: 100%;" class="_div data-v-350cb141">
                <view class="_div data-v-350cb141 na-name">{{miaoyiUser.uname}}</view>
                <view class="_div data-v-350cb141 na-tag">
                  <i-tag wx:if="{{miaoyiUser.ustatus}}" class="_i-tag data-v-350cb141 i-tags" name="单个标签" color="yellow"> VIP </i-tag>
                  <i-tag wx:else class="_i-tag data-v-350cb141 i-tags" name="单个标签" color="yellow"> 会员 </i-tag>
                </view>
                <view class="_div data-v-350cb141 na-point">积分:{{miaoyiUser.upoints}}</view>
              </view>
            </view>
          </view>
        </view>
        <view class="_div data-v-350cb141 info-top-content-co"><button class="_button data-v-350cb141 get-points" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">消费明细</button></view>
      </view>
    </view>
    <view class="_div data-v-350cb141">
      <view class="_div data-v-350cb141 member-money">
        <view class="_div data-v-350cb141 wo-qian">
          <view class="_div data-v-350cb141 yu-e">余额:</view>
          <view class="_div data-v-350cb141 sheng-qian">{{miaoyiUser.umemberMoney}}元</view>
        </view>
      </view>
      <view style="height: 15rpx;background-color: whitesmoke" class="_div data-v-350cb141"></view>
      <view class="_div data-v-350cb141 member-cz">
        <i-button bindtap="handleProxy" inline type="error" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_i-button data-v-350cb141">充500送100</i-button>
        <i-button bindtap="handleProxy" inline type="error" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_i-button data-v-350cb141">充1000送200</i-button>
        <i-button bindtap="handleProxy" inline type="error" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_i-button data-v-350cb141">2000元</i-button>
      </view>
      <view style="height: 55rpx" class="_div data-v-350cb141"></view> <button wx:key="index" key="{{index}}" wx:for="{{li}}" wx:for-index="index" wx:for-item="item" class="_button data-v-350cb141">充{{item.bbase}}送{{item.bgive}}</button>
      <view class="_div data-v-350cb141 addInput">
        <view style="width: 90%;" class="_div data-v-350cb141">
          <van-field value="{{money}}" type="number" label="充值金额" placeholder="请输入充值金额" bindchange="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_van-field data-v-350cb141"></van-field>
        </view>
      </view>
    </view>
    <view class="_div data-v-350cb141 member-foot">
      <i-button bindtap="handleProxy" type="error" long="true" data-eventid="{{'5'}}" data-comkey="{{$k}}" class="_i-button data-v-350cb141">充值</i-button>
    </view>
    <van-toast id="van-toast" class="_van-toast data-v-350cb141"></van-toast>
  </view>
</template>